\begin_layout Standard
\begin_inset Formula 
\begin{align}
\sum_{\langle i,j\rangle\in S_{\text{bonds}}}\mathcal{H}_{ij}^{(4)} & =\sum_{\bm{q}}\sum_{m,n,n'=-1,0}\bigg\{\big[\mathcal{J}(f_{\alpha,m}^{(1)},f_{\beta,nn'}^{(3)})\big(N_{\Theta_{i}\Theta_{j}}^{mn}b_{-\bm{q},(\Theta_{j},n)}b_{\bm{q},(\Theta_{j},n')}+\Delta_{\Theta_{i}\Theta_{j}}^{mn}b_{\bm{q},(\Theta_{j},n)}^{\dagger}b_{\bm{q},(\Theta_{j},n')}\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{mn'}b_{\bm{q},(\Theta_{j},n)}^{\dagger}b_{\bm{q},(\Theta_{j},n)}+\bar{N}_{\Theta_{j}\Theta_{j}}^{nn}b_{-\bm{q},(\Theta_{i},m)}b_{\bm{q},(\Theta_{j},n')}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{j}\Theta_{j}}^{nn'}b_{-\bm{q},(\Theta_{i},m)}b_{\bm{q},(\Theta_{j},n)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\Delta_{\Theta_{j}\Theta_{j}}^{nn'}b_{-\bm{q},(\Theta_{i},m)}b_{-\bm{q},(\Theta_{j},n)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\big)+h.c.\big]\\
 & +\big[\mathcal{J}(f_{\alpha,m}^{(1)},\big[f_{\beta,nn'}^{(3)}\big]^{*})\big(N_{\Theta_{i}\Theta_{j}}^{mn'}b_{\bm{q},(\Theta_{j},n)}^{\dagger}b_{\bm{q},(\Theta_{j},n)}+N_{\Theta_{i}\Theta_{j}}^{mn}b_{\bm{q},(\Theta_{j},n')}^{\dagger}b_{\bm{q},(\Theta_{j},n)}\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{mn}b_{\bm{q},(\Theta_{j},n')}^{\dagger}b_{-\bm{q},(\Theta_{j},n)}^{\dagger}+\bar{\Delta}_{\Theta_{j}\Theta_{j}}^{n'n}b_{-\bm{q},(\Theta_{i},m)}b_{\bm{q},(\Theta_{j},n)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{j}\Theta_{j}}^{n'n}b_{-\bm{q},(\Theta_{i},m)}b_{-\bm{q},(\Theta_{j},n)}^{\dagger}e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{j}\Theta_{j}}^{nn}b_{-\bm{q},(\Theta_{i},m)}b_{-\bm{q},(\Theta_{j},n')}^{\dagger}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\big)+h.c.\big]\\
 & +\big[\mathcal{J}(f_{\alpha,nn'}^{(3)},f_{\beta,m}^{(1)})\big(\bar{N}_{\Theta_{i}\Theta_{i}}^{nn}b_{-\bm{q},(\Theta_{i},n')}b_{\bm{q},(\Theta_{j},m)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{i}\Theta_{i}}^{nn'}b_{-\bm{q},(\Theta_{i},n)}b_{\bm{q},(\Theta_{j},m)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{i}\Theta_{j}}^{nm}b_{-\bm{q},(\Theta_{i},n)}b_{\bm{q},(\Theta_{i},n')}+\Delta_{\Theta_{i}\Theta_{i}}^{nn'}b_{\bm{q},(\Theta_{i},n)}^{\dagger}b_{\bm{q},(\Theta_{j},m)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{nm}b_{\bm{q},(\Theta_{i},n)}^{\dagger}b_{\bm{q},(\Theta_{i},n')}+\Delta_{\Theta_{i}\Theta_{j}}^{n'm}b_{\bm{q},(\Theta_{i},n)}^{\dagger}b_{\bm{q},(\Theta_{i},n)}\big)+h.c.\big]\\
 & +\big[\mathcal{J}(f_{\alpha,nn'}^{(3)},\big[f_{\beta,m}^{(1)}\big]^{*})\big(\bar{N}_{\Theta_{i}\Theta_{i}}^{nn}b_{-\bm{q},(\Theta_{i},n')}b_{-\bm{q},(\Theta_{j},m)}^{\dagger}e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{i}\Theta_{i}}^{nn'}b_{-\bm{q},(\Theta_{i},n)}b_{-\bm{q},(\Theta_{j},m)}^{\dagger}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{\Delta}_{\Theta_{i}\Theta_{j}}^{nm}b_{-\bm{q},(\Theta_{i},n)}b_{\bm{q},(\Theta_{i},n')}+\Delta_{\Theta_{i}\Theta_{i}}^{nn'}b_{\bm{q},(\Theta_{i},n)}^{\dagger}b_{-\bm{q},(\Theta_{j},m)}^{\dagger}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +N_{\Theta_{i}\Theta_{j}}^{nm}b_{\bm{q},(\Theta_{i},n)}^{\dagger}b_{\bm{q},(\Theta_{i},n')}+N_{\Theta_{i}\Theta_{j}}^{n'm}b_{\bm{q},(\Theta_{i},n)}^{\dagger}b_{\bm{q},(\Theta_{i},n)}\big)+h.c.\big]\bigg\}\\
 & +2\sum_{\bm{q}}\sum_{m,m'=-1,0}\sum_{n.n'=-1,0}\bigg\{\mathcal{J}(f_{\alpha,mm'}^{(2)},f_{\beta,nn'}^{(2)})\big(\bar{N}_{\Theta_{i}\Theta_{i}}^{mm'}b_{\bm{q},(\Theta_{j},n)}^{\dagger}b_{\bm{q},(\Theta_{j},n')}\\
 & +\bar{\Delta}_{\Theta_{i}\Theta_{j}}^{mn}b_{-\bm{q},(\Theta_{i},m')}b_{\bm{q},(\Theta_{j},n')}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{i}\Theta_{j}}^{mn'}b_{-\bm{q},(\Theta_{i},m')}b_{\bm{q},(\Theta_{j},n)}e^{i\bm{q}\cdot\bm{\delta}_{ij}}+N_{\Theta_{i}\Theta_{j}}^{m'n}b_{\bm{q},(\Theta_{i},m)}^{\dagger}b_{\bm{q},(\Theta_{j},n')}e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{m'n'}b_{\bm{q},(\Theta_{i},m)}^{\dagger}b_{-\bm{q},(\Theta_{j},n)}^{\dagger}e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{j}\Theta_{j}}^{nn'}b_{\bm{q},(\Theta_{i},m)}^{\dagger}b_{\bm{q},(\Theta_{i},m')}\big)+h.c.\bigg\}
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Consider corrections to the 
\begin_inset Formula $1/N\mathcal{S}$
\end_inset

 order, the quartic vertex function is given by the prefactor of the term
 
\begin_inset Formula $\beta_{\Upsilon,\bm{q}}^{\dagger}\beta_{\Upsilon,\bm{q}}$
\end_inset

.
 Applying Bogoliubov transforms on above results, we obtain the expression
 for the quartic vertex, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
V^{(4)}(\Upsilon,\bm{q}) & =\sum_{\bm{q}}\sum_{m,n,n'=-1,0}\bigg\{\big[\mathcal{J}(f_{\alpha,m}^{(1)},f_{\beta,nn'}^{(3)})\big(N_{\Theta_{i}\Theta_{j}}^{mn}\big[\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{mn}\big[\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})+\Delta_{\Theta_{i}\Theta_{j}}^{mn'}\big[\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})\\
 & +\bar{N}_{\Theta_{j}\Theta_{j}}^{nn}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{j}\Theta_{j}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\Delta_{\Theta_{j}\Theta_{j}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\big)+h.c.\big]\\
 & +\big[\mathcal{J}(f_{\alpha,m}^{(1)},\big[f_{\beta,nn'}^{(3)}\big]^{*})\big(N_{\Theta_{i}\Theta_{j}}^{mn'}\big[\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})\\
 & +N_{\Theta_{i}\Theta_{j}}^{mn}\big[\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})+\Delta_{\Theta_{i}\Theta_{j}}^{mn}\big[\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{21}(\bm{q})\\
 & +\bar{\Delta}_{\Theta_{j}\Theta_{j}}^{n'n}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{j}\Theta_{j}}^{n'n}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{21}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{j}\Theta_{j}}^{nn}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{21}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\big)+h.c.\big]\\
 & +\big[\mathcal{J}(f_{\alpha,nn'}^{(3)},f_{\beta,m}^{(1)})\big(\bar{N}_{\Theta_{i}\Theta_{i}}^{nn}\big[\mathcal{U}_{(\Theta_{i},n'),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{i}\Theta_{i}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{i}\Theta_{j}}^{nm}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},n'),\Upsilon}^{11}(\bm{q})+\Delta_{\Theta_{i}\Theta_{i}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{nm}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},n'),\Upsilon}^{11}(\bm{q})+\Delta_{\Theta_{i}\Theta_{j}}^{n'm}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big)+h.c.\big]\\
 & +\big[\mathcal{J}(f_{\alpha,nn'}^{(3)},\big[f_{\beta,m}^{(1)}\big]^{*})\big(\bar{N}_{\Theta_{i}\Theta_{i}}^{nn}\big[\mathcal{U}_{(\Theta_{i},n'),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{21}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{i}\Theta_{i}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{21}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{\Delta}_{\Theta_{i}\Theta_{j}}^{nm}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},n'),\Upsilon}^{11}(\bm{q})+\Delta_{\Theta_{i}\Theta_{i}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{21}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +N_{\Theta_{i}\Theta_{j}}^{nm}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},n'),\Upsilon}^{11}(\bm{q})+N_{\Theta_{i}\Theta_{j}}^{n'm}\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{q})\big)+h.c.\big]\bigg\}\\
 & +2\sum_{\bm{q}}\sum_{m,m'=-1,0}\sum_{n.n'=-1,0}\bigg\{\mathcal{J}(f_{\alpha,mm'}^{(2)},f_{\beta,nn'}^{(2)})\big(\bar{N}_{\Theta_{i}\Theta_{i}}^{mm'}\big[\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})\\
 & +\bar{\Delta}_{\Theta_{i}\Theta_{j}}^{mn}\big[\mathcal{U}_{(\Theta_{i},m'),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\bar{N}_{\Theta_{i}\Theta_{j}}^{mn'}\big[\mathcal{U}_{(\Theta_{i},m'),\Upsilon}^{21}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}+N_{\Theta_{i}\Theta_{j}}^{m'n}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n'),\Upsilon}^{11}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}\\
 & +\Delta_{\Theta_{i}\Theta_{j}}^{m'n'}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{j},n),\Upsilon}^{21}(\bm{q})e^{i\bm{q}\cdot\bm{\delta}_{ij}}+\bar{N}_{\Theta_{j}\Theta_{j}}^{nn'}\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{q})\big]^{*}\mathcal{U}_{(\Theta_{i},m'),\Upsilon}^{11}(\bm{q})\big)+h.c.\bigg\}
\end{align}

