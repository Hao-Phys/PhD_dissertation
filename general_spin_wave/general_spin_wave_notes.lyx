\begin_layout Standard
The on-site term does not contribute to the quartic Hamiltonian.
 Consider a generic quartic term, the Wick theorem tells us that the vacuum
 expectation of which consists of two-contraction terms, two-normal-ordered
 terms, and one-contration-one-normal-ordered terms.
 Terms of the first type correct the classical energy, while those of the
 second type contribute the boson scattering vertex to the leading order.
 As we discussed before, terms of the third type correspond to the Hartree-Fock
 corrections of boson energies, which is the main focus of this manuscript.
 
\end_layout

\begin_layout Standard
As a starting point, we define following mean-field quantities, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
N_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}b_{\Theta_{j},n}^{\dagger}\rangle,\\
\bar{N}_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}^{\dagger}b_{\Theta_{j},n}\rangle,\\
\Delta_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}b_{\Theta_{j},n}\rangle,\\
\bar{\Delta}_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}^{\dagger}b_{\Theta_{j},n}^{\dagger}\rangle,
\end{align}

\end_inset


\end_layout

\begin_layout Standard
where 
\begin_inset Formula $\Theta_{i,j}$
\end_inset

 are the sublattice indices and 
\begin_inset Formula $m,n$
\end_inset

 are the flavor indices.
 They can be calculated as following, 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
N_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}b_{\Theta_{j},n}^{\dagger}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\langle b_{\bm{k},(\Theta_{i},m)}b_{\bm{k},(\Theta_{j},n)}^{\dagger}e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon,\Upsilon'}\bigg\langle\big(\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{k})\beta_{\Upsilon,\bm{k}}+\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bar{\bm{k}})\big]^{*}\beta_{\Upsilon,\bar{\bm{k}}}^{\dagger}\big)\\
 & \big(\mathcal{U}_{(\Theta_{j},m),\Upsilon'}^{21}(\bar{\bm{k}})\beta_{\Upsilon',\bar{\bm{k}}}+\big[\mathcal{U}_{(\Theta_{j},m),\Upsilon'}^{11}(\bm{k})\big]^{*}\beta_{\Upsilon',\bm{k}}^{\dagger}\big)e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\bigg\rangle\\
 & \xrightarrow{\beta\rightarrow\infty}\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon}\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{k})\big[\mathcal{U}_{(\Theta_{j},m),\Upsilon}^{11}(\bm{k})\big]^{*}e^{-i\bm{k}\cdot\bm{\delta}_{ij}},
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\bar{N}_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}^{\dagger}b_{\Theta_{j},n}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\langle b_{\bar{\bm{k}},(\Theta_{i},m)}^{\dagger}b_{\bar{\bm{k}},(\Theta_{j},n)}e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon,\Upsilon'}\bigg\langle\big(\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{k})\beta_{\Upsilon,\bm{k}}+\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bar{\bm{k}})\big]^{*}\beta_{\Upsilon,\bar{\bm{k}}}^{\dagger}\big)\\
 & \big(\mathcal{U}_{(\Theta_{i},n),\Upsilon'}^{11}(\bar{\bm{k}})\beta_{\Upsilon',\bar{\bm{k}}}+\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon'}^{21}(\bm{k})\big]^{*}\beta_{\Upsilon',\bm{k}}^{\dagger}\big)e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\bigg\rangle\\
 & \xrightarrow{\beta\rightarrow\infty}\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon}\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{k})\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{k})\big]^{*}e^{-i\bm{k}\cdot\bm{\delta}_{ij}},
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\Delta_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}b_{\Theta_{j},n}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\langle b_{\bm{k},(\Theta_{i},m)}b_{\bar{\bm{k}},(\Theta_{j},n)}e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon,\Upsilon'}\bigg\langle\big(\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{k})\beta_{\Upsilon,\bm{k}}+\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bar{\bm{k}})\big]^{*}\beta_{\Upsilon,\bar{\bm{k}}}^{\dagger}\big)\\
 & \big(\mathcal{U}_{(\Theta_{i},n),\Upsilon'}^{11}(\bar{\bm{k}})\beta_{\Upsilon',\bar{\bm{k}}}+\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{k})\big]^{*}\beta_{\Upsilon,\bm{k}}^{\dagger}\big)e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\bigg\rangle\\
 & \xrightarrow{\beta\rightarrow\infty}\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon}\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bm{k})\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{21}(\bm{k})\big]^{*}e^{-i\bm{k}\cdot\bm{\delta}_{ij}},
\end{align}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align}
\bar{\Delta}_{\Theta_{i}\Theta_{j}}^{mn} & \equiv\frac{1}{N_{u}}\sum_{\langle i,j\rangle}\langle b_{\Theta_{i},m}^{\dagger}b_{\Theta_{j},n}^{\dagger}\rangle\\
 & =\frac{1}{N_{u}}\sum_{\bm{k}}\langle b_{\bar{\bm{k}},(\Theta_{i},m)}^{\dagger}b_{\bm{k},(\Theta_{j},n)}^{\dagger}e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\rangle\\
 & \frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon,\Upsilon'}\bigg\langle\big(\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{k})\beta_{\Upsilon,\bm{k}}+\big[\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{11}(\bar{\bm{k}})\big]^{*}\beta_{\Upsilon,\bar{\bm{k}}}^{\dagger}\big)\\
 & \big(\mathcal{U}_{(\Theta_{i},n),\Upsilon'}^{21}(\bar{\bm{k}})\beta_{\Upsilon',\bar{\bm{k}}}+\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon'}^{11}(\bm{k})\big]^{*}\beta_{\Upsilon',\bm{k}}^{\dagger}\big)e^{-i\bm{k}\cdot\bm{\delta}_{ij}}\bigg\rangle\\
 & \xrightarrow{\beta\rightarrow\infty}\frac{1}{N_{u}}\sum_{\bm{k}}\sum_{\Upsilon}\mathcal{U}_{(\Theta_{i},m),\Upsilon}^{21}(\bm{k})\big[\mathcal{U}_{(\Theta_{i},n),\Upsilon}^{11}(\bm{k})\big]^{*}e^{-i\bm{k}\cdot\bm{\delta}_{ij}}.
\end{align}

\end_inset


\end_layout

\begin_layout Standard
Upon summation over all symmetry inequivalent bonds, the cubic Hamiltonian
 in momentum space can be written as
